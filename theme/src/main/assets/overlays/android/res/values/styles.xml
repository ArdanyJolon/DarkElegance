    <!--Resolver correction-->
    <style name="Theme.DeviceDefault.Resolver" parent="@style/Theme.Material.Light">
        <item name="android:windowBackground">@*common:color/tr</item>
        <item name="android:textColorPrimary">@*common:color/wh</item>
        <item name="android:textColorSecondary">@*common:color/sk</item>
        <item name="windowNoTitle">true</item>
        <item name="windowIsTranslucent">true</item>
        <item name="windowContentOverlay">@null</item>
        <item name="backgroundDimEnabled">true</item>
        <item name="listPreferredItemPaddingStart">?dialogPreferredPadding</item>
        <item name="listPreferredItemPaddingEnd">?dialogPreferredPadding</item>
        <item name="windowTranslucentStatus">false</item>
        <item name="windowTranslucentNavigation">false</item>
        <item name="colorControlActivated">?colorControlHighlight</item>
        <item name="windowDrawsSystemBarBackgrounds">false</item>
    </style>

